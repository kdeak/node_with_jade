extends layout

block content
  h1= title
  p This is Sequelize greeting you :)

  h2 Creating a new jelenlet
  form(action="/jelenletek/create", method="post")
    input(type="text", name="name")
    input(type="submit")

  h2 Jelenletek

  ul
  each jelenlet in jelenletek
    li
      = jelenlet.name
      | &nbsp;
      | (
      a(href="/jelenletek/" + jelenlet.id + "/destroy") Destroy
      | )
      ul